<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
	<id>Havit.Web.Bootstrap</id>
	<version>1.7.0</version>
	<title>Havit.Web.Bootstrap</title>
	<authors>HAVIT</authors>
	<owners>HAVIT</owners>
	<iconUrl>https://www.gravatar.com/avatar/e59ffb10448162f0c6d6b5ae3fecd0ce?s=64</iconUrl>
	<licenseUrl>https://www.apache.org/licenses/LICENSE-2.0</licenseUrl>
	<description>
	  Installs Havit.Web.Bootstrap assembly and Bootstrap LESS files (via Twitter.Bootstrap.Less NuGet) to project.
	</description>
	<language>en-US</language>
	<projectUrl>http://hfw.havit.local/Havit.Web.Bootstrap.Tutorial</projectUrl>
	<requireLicenseAcceptance>false</requireLicenseAcceptance>
	<!--<licenseUrl>http://opensource.org/licenses/Apache-2.0</licenseUrl>-->
	<copyright>Copyright HAVIT, s.r.o. 2018</copyright>
	<dependencies>
	  <group targetFramework="net46">
		<dependency id="jQuery" version="1.11.0" />
		<dependency id="Twitter.Bootstrap.Less" version="[3.2.0.1,4)" />
		<dependency id="Microsoft.AspNet.ScriptManager.WebForms" version="5.0.0" />
		<dependency id="toastr" version="2.0.1" />
	  </group>
	</dependencies>
	<tags>HAVIT ASP.NET Web WebForms Bootstrap Controls</tags>
  </metadata>
  <files>
	<file src="..\Havit.Web\Bootstrap\content\less\*.less" target="content\Content\havit.web.bootstrap\" />
	<file src="..\Havit.Web\Bootstrap\content\js\*.js" target="content\Scripts\havit.web.bootstrap\" />
	<file src="Havit.Web.Bootstrap\content\Web.config.install.xdt" target="content" />
	<file src="Havit.Web.Bootstrap\content\Web.config.uninstall.xdt" target="content" />
  </files>
</package>