<?xml version="1.0" encoding="utf-8" ?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <glimpse xdt:Transform="Remove" />

  <system.diagnostics>
    <sources>
      <source name="DbConnector Command Execution Trace">
        <listeners>
          <add name="GlimpseDbConnectorTraceListener"  xdt:Transform="Remove" xdt:Locator="Match(name)" />
        </listeners>
      </source>
    </sources>
  </system.diagnostics>
  
</configuration>